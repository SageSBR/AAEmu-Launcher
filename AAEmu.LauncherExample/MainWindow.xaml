<Window x:Class="AAEmu.LauncherExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AAEmu.LauncherExample"
        mc:Ignorable="d"
        Title="AAEmu.Launcher" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tUserName" HorizontalAlignment="Left" Margin="20,23,0,0" Text="Test" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Height="35" FontSize="24"/>
        <TextBox x:Name="tPassword" HorizontalAlignment="Left" Margin="20,63,0,0" Text="test" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Height="35" FontSize="24" UseLayoutRounding="False"/>
        <Button x:Name="btnPlay" Content="Button" HorizontalAlignment="Left" Margin="340,21,0,0" VerticalAlignment="Top" Height="37" Width="140" Click="btnPlay_Click"/>
    </Grid>
</Window>
